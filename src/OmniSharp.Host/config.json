  "IgnoredCodeIssues": [
    "^Keyword 'private' is redundant. This is the default modifier.$",
    ".* should not separate words with an underscore.*"
  ],

    "TestCommands": {
        "All": "dotnet test {{AssemblyPath}}",
        "Fixture": "dotnet test {{AssemblyPath}} --filter={{TypeName}}",
        "Single": "dotnet test {{AssemblyPath}} --filter={{TypeName}}"
    },

  "MSBuildPath": {
    "Path": "C://Tools//dotnet//"
  },

  "TextEditorOptions": {
    "tabsToSpaces": true,
    "tabSize": 4,
    "indentSize": 4,
    "continuationIndent": 4,
    "labelIndent": 0,
    /* "eolMarker": "\r\n", */
    "indentBlankLines": false,
    "wrapLineLength": 80
  },

